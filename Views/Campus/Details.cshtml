@model SP23_G21_SHSMS.Models.SHSMS.Campus

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Campus</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CampusName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CampusName)
        </dd>
        @*<dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ConnectionString)
        </dt>*@
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ConnectionString)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd class = "col-sm-10">
            @{
                string s = Model.Status == true ? "Active" : "Deactive";
            }
            @s
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Edu)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Edu.EduName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.CampusId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
